      <Description>Manufacturer specific product designation or model name.</Description>
      <Version>1.0.0</Version>
    </AttributeType>
    <AttributeType Name="partNumber" AttributeDataType="xs:string" ID="4778e1b2-d9ec-4945-b5ed-4abd807e2d7e">
      <Description>Part identifier assigned to a component or product.</Description>
      <Version>1.0.0</Version>
    </AttributeType>
    <AttributeType Name="serialNumber" AttributeDataType="xs:string" ID="81d2caa2-36ca-40f2-bc19-a266f691bf33">
