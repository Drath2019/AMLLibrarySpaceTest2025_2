  <RoleClassLib Name="MaterialHandlingProcessClassLib" ChangeMode="change">
    <Description>Role class library for material flow processes. Mostly adopted from VDI 2860.</Description>
    <Version>1.4</Version>
    <RoleClass Name="Behavior" RefBaseClassPath="AutomationMLBaseLibraries@AutomationMLBaseRoleClassLib/AutomationMLBaseRole/Process" ChangeMode="create">
      <Description>Defines handling processes for the accumulation, merging, sorting, dividing, etc.</Description>
      <RoleClass Name="Accumulation" RefBaseClassPath="MaterialHandlingProcessClassLib/Behavior" ChangeMode="create">
        <Description>Defines congestion behavior</Description>
      </RoleClass>
      <RoleClass Name="Merging" RefBaseClassPath="MaterialHandlingProcessClassLib/Behavior" ChangeMode="create">
        <Description>Defines merging of multiple material handling objects</Description>
        <AdditionalInformation>see VDI 2860 (Zusammenführen)</AdditionalInformation>
        <Attribute Name="PriorityRule" AttributeDataType="xs:string">
          <Constraint Name="Priority">
            <NominalScaledType>
              <RequiredValue>FIFO</RequiredValue>
              <RequiredValue>LIFO</RequiredValue>
              <RequiredValue>Sequence</RequiredValue>
              <RequiredValue>PriorityByProduct</RequiredValue>
            </NominalScaledType>
          </Constraint>
        </Attribute>
      </RoleClass>
      <RoleClass Name="Sorting" RefBaseClassPath="MaterialHandlingProcessClassLib/Behavior" ChangeMode="create">
        <Description>Defines the sorting process in material handling</Description>
        <AdditionalInformation>see VDI 2860 (Sortieren)</AdditionalInformation>
      </RoleClass>
      <RoleClass Name="Dividing" RefBaseClassPath="MaterialHandlingProcessClassLib/Behavior" ChangeMode="create">
        <Description>A general process role to define how to create subsets from an amount</Description>
        <AdditionalInformation>see VDI 2860 (Teilen)</AdditionalInformation>
        <RoleClass Name="Partition" RefBaseClassPath="MaterialHandlingProcessClassLib/Behavior/Dividing" ChangeMode="create">
          <Description>Defines a process to form subsets with defined size</Description>
          <AdditionalInformation>see VDI 2860 (Abteilen)</AdditionalInformation>
        </RoleClass>
        <RoleClass Name="Distribution" RefBaseClassPath="MaterialHandlingProcessClassLib/Behavior/Dividing" ChangeMode="create">
          <Description>Defines a process to form subsets with defined size and transfer them to a defined place</Description>
          <AdditionalInformation>see VDI 2860 (Zuteilen)</AdditionalInformation>
        </RoleClass>
        <RoleClass Name="Branching" RefBaseClassPath="MaterialHandlingProcessClassLib/Behavior/Dividing" ChangeMode="create">
          <Description>Defines a process to split a material flow to smaller flows</Description>
          <AdditionalInformation>see VDI 2860(Verzweigen)</AdditionalInformation>
        </RoleClass>
      </RoleClass>
    </RoleClass>
    <RoleClass Name="Moving" RefBaseClassPath="AutomationMLBaseLibraries@AutomationMLBaseRoleClassLib/AutomationMLBaseRole/Process" ChangeMode="create">
      <Description>Defines a change in the spatial arrangement of a body</Description>
      <AdditionalInformation>see VDI 2860(Bewegen)</AdditionalInformation>
      <Attribute Name="RotationOfTransportUnit" AttributeDataType="xs:string">
        <Description>Defines the rotation of a transport unit in the process as Lengthwise, Crosswise or Random.</Description>
        <Constraint Name="Rotation">
          <NominalScaledType>
            <RequiredValue>Lengthwise</RequiredValue>
            <RequiredValue>Crosswise</RequiredValue>
            <RequiredValue>Random</RequiredValue>
          </NominalScaledType>
        </Constraint>
      </Attribute>
      <Attribute Name="LateralPosition" AttributeDataType="xs:string">
        <Description>Defines the lateral position of a transport unit relative to the transport direction as LeftHand, RightHand, Centered or Random.</Description>
        <Constraint Name="Position">
          <NominalScaledType>
            <RequiredValue>LeftHand</RequiredValue>
            <RequiredValue>RightHand</RequiredValue>
            <RequiredValue>Centered</RequiredValue>
            <RequiredValue>Random</RequiredValue>
          </NominalScaledType>
        </Constraint>
      </Attribute>
      <RoleClass Name="Turning" RefBaseClassPath="MaterialHandlingProcessClassLib/Moving" ChangeMode="create">
        <Description>Changing the orientation of the body without changing the position</Description>
        <AdditionalInformation>see VDI 2860 (Drehen)</AdditionalInformation>
      </RoleClass>
      <RoleClass Name="Translating" RefBaseClassPath="MaterialHandlingProcessClassLib/Moving" ChangeMode="create">
        <Description>Moving a body in a linear direction without changing the orientation</Description>
        <AdditionalInformation>see VDI 2860 (Verschieben)</AdditionalInformation>
      </RoleClass>
      <RoleClass Name="Swiveling" RefBaseClassPath="MaterialHandlingProcessClassLib/Moving" ChangeMode="create">
        <Description>Rotation of a body over a center of an axis, which is not within the body, resulting an orientation and position change</Description>
        <AdditionalInformation>see VDI 2860 (Schwenken)</AdditionalInformation>
      </RoleClass>
      <RoleClass Name="Orientating" ChangeMode="create" RefBaseClassPath="MaterialHandlingProcessClassLib/Moving">
        <Description>Change of the orientation from an undefined to a defined one. Position change of the body is disregarded.</Description>
        <AdditionalInformation>see VDI 2860 (Orientieren)</AdditionalInformation>
      </RoleClass>
      <RoleClass Name="Positioning" RefBaseClassPath="MaterialHandlingProcessClassLib/Moving" ChangeMode="create">
        <Description>Moving a body to a defined position. Orientation of the body is disregarded.</Description>
        <AdditionalInformation>see VDI 2860 (Positionieren)</AdditionalInformation>
      </RoleClass>
      <RoleClass Name="Arranging" RefBaseClassPath="MaterialHandlingProcessClassLib/Moving" ChangeMode="create">
        <Description>Moving a body from an undefined place to a defined orientation and position providing arrangement.</Description>
        <AdditionalInformation>see VDI 2860 (Ordnen)</AdditionalInformation>
      </RoleClass>
      <RoleClass Name="Directing" RefBaseClassPath="MaterialHandlingProcessClassLib/Moving" ChangeMode="create">
        <Description>Moving a body on a defined path from a defined place to another defined place</Description>
        <AdditionalInformation>see VDI 2860 (Führen)</AdditionalInformation>
      </RoleClass>
      <RoleClass Name="HandingOver" RefBaseClassPath="MaterialHandlingProcessClassLib/Moving" ChangeMode="create">
        <Description>Moving a body on an undefined path from a defined place to another defined place</Description>
        <AdditionalInformation>see VDI 2860 (Weitergeben)</AdditionalInformation>
      </RoleClass>
      <RoleClass Name="Conveying" RefBaseClassPath="MaterialHandlingProcessClassLib/Moving" ChangeMode="create">
        <Description>Moving a body from any place to another place. Orientation and position of the body during the movement is not necessarily defined.</Description>
        <AdditionalInformation>see VDI 2860 (Fördern)</AdditionalInformation>
        <RoleClass Name="ContinuousConveying" RefBaseClassPath="MaterialHandlingProcessClassLib/Moving/Conveying" ChangeMode="create">
          <Description>Conveying process that create a continuous conveyance flow.</Description>
        </RoleClass>
        <RoleClass Name="DiscontinuousConveying" RefBaseClassPath="MaterialHandlingProcessClassLib/Moving/Conveying" ChangeMode="create">
          <Description>Conveying process to move materials in individual working cycles between take-up and delivery points.</Description>
        </RoleClass>
      </RoleClass>
    </RoleClass>
    <RoleClass Name="Securing" RefBaseClassPath="AutomationMLBaseLibraries@AutomationMLBaseRoleClassLib/AutomationMLBaseRole/Process" ChangeMode="create">
      <Description>A general role to define securing a defined status</Description>
      <AdditionalInformation>see VDI 2860 (Sichern)</AdditionalInformation>
      <RoleClass Name="Holding" RefBaseClassPath="MaterialHandlingProcessClassLib/Securing" ChangeMode="create">
        <Description>Securing the orientation and position of a body</Description>
        <AdditionalInformation>see VDI 2860 (Halten)</AdditionalInformation>
      </RoleClass>
      <RoleClass Name="Loosening" RefBaseClassPath="MaterialHandlingProcessClassLib/Securing" ChangeMode="create">
        <Description>Loosening the orientation and position of a body, reversal of holding.</Description>
        <AdditionalInformation>see VDI 2860 (Lösen)</AdditionalInformation>
      </RoleClass>
      <RoleClass Name="Tightening" RefBaseClassPath="MaterialHandlingProcessClassLib/Securing" ChangeMode="create">
        <Description>Securing the orientation and position of a body with applying force</Description>
        <AdditionalInformation>see VDI 2860 (Spannen)</AdditionalInformation>
      </RoleClass>
      <RoleClass Name="Relieving" RefBaseClassPath="MaterialHandlingProcessClassLib/Securing" ChangeMode="create">
        <Description>Relieving the force which is blocking the orientational and positional change, reversal of tightening.</Description>
        <AdditionalInformation>see VDI 2860 (Entspannen)</AdditionalInformation>
      </RoleClass>
    </RoleClass>
    <RoleClass Name="Verification" RefBaseClassPath="AutomationMLBaseLibraries@AutomationMLBaseRoleClassLib/AutomationMLBaseRole/Process" ChangeMode="create">
      <Description>Defines a general role for inspection and measuring of properties and status</Description>
      <AdditionalInformation>see VDI 2860 (Kontrollieren)</AdditionalInformation>
      <RoleClass Name="Inspection" RefBaseClassPath="MaterialHandlingProcessClassLib/Verification" ChangeMode="create">
        <Description>Inspection of properties and status. Admission of information, comparison with target properties, status or decisions.</Description>
        <AdditionalInformation>see VDI 2860 (Prüfen)</AdditionalInformation>
        <RoleClass Name="PresenceCheck" RefBaseClassPath="MaterialHandlingProcessClassLib/Verification/Inspection" ChangeMode="create">
          <Description>Determining if a body is present in a defined place</Description>
          <AdditionalInformation>see VDI 2860 (Anwesenheit prüfen)</AdditionalInformation>
        </RoleClass>
        <RoleClass Name="IdentityCheck" RefBaseClassPath="MaterialHandlingProcessClassLib/Verification/Inspection" ChangeMode="create">
          <Description>Determining if a body satisfies the defined properties</Description>
          <AdditionalInformation>see VDI 2860 (Identität prüfen)</AdditionalInformation>
        </RoleClass>
        <RoleClass Name="ShapeCheck" RefBaseClassPath="MaterialHandlingProcessClassLib/Verification/Inspection" ChangeMode="create">
          <Description>Determining if a body has the defined shape.</Description>
          <AdditionalInformation>see VDI 2860 (Form prüfen)</AdditionalInformation>
        </RoleClass>
        <RoleClass Name="SizeCheck" RefBaseClassPath="MaterialHandlingProcessClassLib/Verification/Inspection" ChangeMode="create">
          <Description>Determining if a body has the defined sizes/measurements.</Description>
          <AdditionalInformation>see VDI 2860 (Größe prüfen)</AdditionalInformation>
        </RoleClass>
        <RoleClass Name="ColorCheck" RefBaseClassPath="MaterialHandlingProcessClassLib/Verification/Inspection" ChangeMode="create">
          <Description>Determining if a body or body areas have the defined colors</Description>
          <AdditionalInformation>see VDI 2860 (Farbe prüfen)</AdditionalInformation>
        </RoleClass>
        <RoleClass Name="WeightCheck" RefBaseClassPath="MaterialHandlingProcessClassLib/Verification/Inspection" ChangeMode="create">
          <Description>Determining if a body has the defined weight</Description>
          <AdditionalInformation>see VDI 2860 (Gewicht prüfen)</AdditionalInformation>
        </RoleClass>
        <RoleClass Name="PositionCheck" RefBaseClassPath="MaterialHandlingProcessClassLib/Verification/Inspection" ChangeMode="create">
          <Description>Determining if a body has the defined position</Description>
          <AdditionalInformation>see VDI 2860 (Position prüfen)</AdditionalInformation>
        </RoleClass>
        <RoleClass Name="OrientationCheck" RefBaseClassPath="MaterialHandlingProcessClassLib/Verification/Inspection" ChangeMode="create">
          <Description>Determining if a body has the defined orientation</Description>
          <AdditionalInformation>see VDI 2860 (Orientatierung prüfen)</AdditionalInformation>
        </RoleClass>
      </RoleClass>
      <RoleClass Name="Measuring" RefBaseClassPath="MaterialHandlingProcessClassLib/Verification" ChangeMode="create">
        <Description>Measuring the values in accordance to reference values</Description>
        <RoleClass Name="Counting" RefBaseClassPath="MaterialHandlingProcessClassLib/Verification/Measuring" ChangeMode="create">
          <Description>Determining the amount of bodies</Description>
          <AdditionalInformation>see VDI 2860 (Zählen)</AdditionalInformation>
        </RoleClass>
        <RoleClass Name="OrientationMeasuring" RefBaseClassPath="MaterialHandlingProcessClassLib/Verification/Measuring" ChangeMode="create">
          <Description>Determining the orientation of a body in accordance to a reference coordinate system</Description>
          <AdditionalInformation>see VDI 2860 (Orientation messen)</AdditionalInformation>
        </RoleClass>
        <RoleClass Name="PositionMeasuring" RefBaseClassPath="MaterialHandlingProcessClassLib/Verification/Measuring" ChangeMode="create">
          <Description>Determining the position of a body in accordance to a reference coordinate system</Description>
          <AdditionalInformation>see VDI 2860 (Position messen)</AdditionalInformation>
        </RoleClass>
      </RoleClass>
    </RoleClass>
    <RoleClass Name="Source&amp;Sink" RefBaseClassPath="AutomationMLBaseLibraries@AutomationMLBaseRoleClassLib/AutomationMLBaseRole/Process" ChangeMode="create">
      <Description>General role to define source and sink processes</Description>
      <RoleClass Name="Source" RefBaseClassPath="MaterialHandlingProcessClassLib/Source&amp;Sink" ChangeMode="create">
        <Description>Defines the source of a material flow</Description>
        <Attribute Name="OutputMode" AttributeDataType="xs:string" ChangeMode="create">
          <Description>Creating transport units randomly or in a synchronized manner</Description>
          <DefaultValue>clocked</DefaultValue>
        </Attribute>
        <Attribute Name="DelayMode" AttributeDataType="xs:string" ChangeMode="create">
          <Description>Specified or determined randomly within a selectable time range</Description>
          <DefaultValue>fix</DefaultValue>
        </Attribute>
        <Attribute Name="CycleTime" AttributeDataType="xs:positiveInteger" Unit="s" ChangeMode="create">
          <Description>Cycle time</Description>
        </Attribute>
        <Attribute Name="DelayTime" AttributeDataType="xs:positiveInteger" Unit="s" ChangeMode="create">
          <Description>Delay time</Description>
        </Attribute>
        <Attribute Name="RotationOfTransportUnit" AttributeDataType="xs:string">
          <Description>Defines the rotation of a transport unit in the process as Lengthwise, Crosswise or Random.</Description>
          <Constraint Name="Rotation">
            <NominalScaledType>
              <RequiredValue>Lengthwise</RequiredValue>
              <RequiredValue>Crosswise</RequiredValue>
              <RequiredValue>Random</RequiredValue>
            </NominalScaledType>
          </Constraint>
        </Attribute>
        <Attribute Name="LateralPosition" AttributeDataType="xs:string">
          <Description>Defines the lateral position of a transport unit relative to the transport direction as LeftHand, RightHand, Centered or Random.</Description>
          <Constraint Name="Position">
            <NominalScaledType>
              <RequiredValue>LeftHand</RequiredValue>
              <RequiredValue>RightHand</RequiredValue>
              <RequiredValue>Centered</RequiredValue>
              <RequiredValue>Random</RequiredValue>
            </NominalScaledType>
          </Constraint>
        </Attribute>
      </RoleClass>
      <RoleClass Name="Sink" RefBaseClassPath="MaterialHandlingProcessClassLib/Source&amp;Sink" ChangeMode="create">
        <Description>Defines the sink of a material flow</Description>
        <Attribute Name="InputMode" AttributeDataType="xs:string" ChangeMode="create">
          <Description>Receiving transport units randomly or in a synchronized manner</Description>
          <DefaultValue>clocked</DefaultValue>
        </Attribute>
        <Attribute Name="DelayMode" AttributeDataType="xs:string" ChangeMode="create">
          <Description>Specified or determined randomly within a selectable time range</Description>
          <DefaultValue>fix</DefaultValue>
        </Attribute>
        <Attribute Name="CycleTime" AttributeDataType="xs:positiveInteger" Unit="s" ChangeMode="create">
          <Description>Cycle time</Description>
        </Attribute>
        <Attribute Name="DelayTime" AttributeDataType="xs:positiveInteger" Unit="s" ChangeMode="create">
          <Description>Delay time</Description>
        </Attribute>
        <Attribute Name="RotationOfTransportUnit" AttributeDataType="xs:string">
          <Description>Defines the rotation of a transport unit in the process as Lengthwise, Crosswise or Random.</Description>
          <Constraint Name="Rotation">
            <NominalScaledType>
              <RequiredValue>Lengthwise</RequiredValue>
              <RequiredValue>Crosswise</RequiredValue>
              <RequiredValue>Random</RequiredValue>
            </NominalScaledType>
          </Constraint>
        </Attribute>
        <Attribute Name="LateralPosition" AttributeDataType="xs:string">
          <Description>Defines the rotation of a transport unit in the process as Lengthwise, Crosswise or Random.</Description>
          <Constraint Name="Position">
            <NominalScaledType>
              <RequiredValue>LeftHand</RequiredValue>
              <RequiredValue>RightHand</RequiredValue>
              <RequiredValue>Centered</RequiredValue>
              <RequiredValue>Random</RequiredValue>
            </NominalScaledType>
          </Constraint>
        </Attribute>
      </RoleClass>
    </RoleClass>
    <RoleClass Name="TransportUnitProcess" RefBaseClassPath="AutomationMLBaseLibraries@AutomationMLBaseRoleClassLib/AutomationMLBaseRole/Process" ChangeMode="create">
      <Description>A general class to define transport unit processes</Description>
      <Version>1.2</Version>
      <Attribute Name="ProcessTime" AttributeDataType="xs:positiveInteger" Unit="s" ChangeMode="create">
        <Description>The period during which the transport unit process executed.</Description>
        <DefaultValue>0</DefaultValue>
      </Attribute>
      <RoleClass Name="Loading" RefBaseClassPath="MaterialHandlingProcessClassLib/TransportUnitProcess" ChangeMode="create">
        <Description>Defines a transport unit loading process</Description>
      </RoleClass>
      <RoleClass Name="Unloading" RefBaseClassPath="MaterialHandlingProcessClassLib/TransportUnitProcess" ChangeMode="create">
        <Description>Defines a transport unit unloading process</Description>
      </RoleClass>
      <RoleClass Name="Transloading" RefBaseClassPath="MaterialHandlingProcessClassLib/TransportUnitProcess" ChangeMode="create">
        <Description>Transloading is the process of transferring a transport unit from one mode of transportation to another.</Description>
        <Version>1.0</Version>
      </RoleClass>
      <RoleClass Name="OrderPicking" RefBaseClassPath="MaterialHandlingProcessClassLib/TransportUnitProcess" ChangeMode="create">
        <Description>Order picking stands for the compilation of transport units according to given orders from an available complete assortment.</Description>
        <Version>1.0</Version>
      </RoleClass>
      <RoleClass Name="Carrying" RefBaseClassPath="MaterialHandlingProcessClassLib/TransportUnitProcess" ChangeMode="change">
        <Description>Defines the process of carrying transport units with a component or a load carrier.</Description>
        <Version>1.1</Version>
        <Attribute Name="LoadCapacity" AttributeDataType="xs:double" Unit="kg">
          <Description>Defines transport unit capacity in kilograms.</Description>
        </Attribute>
        <Attribute Name="AmountCapacity" AttributeDataType="xs:positiveInteger" Unit="pce">
          <Description>Defines transport unit capacity in pieces.</Description>
        </Attribute>
      </RoleClass>
    </RoleClass>
    <RoleClass Name="Storing" RefBaseClassPath="AutomationMLBaseLibraries@AutomationMLBaseRoleClassLib/AutomationMLBaseRole/Process" ChangeMode="create">
      <Description>Storing is according to VDI 2411 every planned interruption in the material flow.</Description>
      <Version>1.0</Version>
      <Attribute Name="CapacityAmount" AttributeDataType="xs:positiveInteger" Unit="pce">
        <Description>Capacity defined in amount of transport units.</Description>
      </Attribute>
      <Attribute Name="CapacityLength" AttributeDataType="xs:double" Unit="m">
        <Description>Defines the length which allows the storage of transport units with different sizes.</Description>
      </Attribute>
      <Attribute Name="PriorityRule" AttributeDataType="xs:string">
        <Description>Defines the priority rule as FIFO, LIFO, Sequence or PriorityByProduct.</Description>
        <Constraint Name="Priority">
          <NominalScaledType>
            <RequiredValue>FIFO</RequiredValue>
            <RequiredValue>LIFO</RequiredValue>
            <RequiredValue>Sequence</RequiredValue>
            <RequiredValue>PriorityByProduct</RequiredValue>
          </NominalScaledType>
        </Constraint>
      </Attribute>
      <RoleClass Name="Stocking" RefBaseClassPath="MaterialHandlingProcessClassLib/Storing" ChangeMode="create">
        <Description>Storing transport units for longer periods.</Description>
        <Attribute Name="MediumSingleCycleTime" AttributeDataType="xs:positiveInteger" Unit="s">
          <Description>Medium time that is spent to load a transport unit to storage or unload a transport unit from storage.</Description>
        </Attribute>
        <Attribute Name="MediumDoubleCycleTime" AttributeDataType="xs:positiveInteger" Unit="s">
          <Description>Medium time that is spent to load a transport unit to storage, pick up another transport unit from storage and unload.</Description>
        </Attribute>
      </RoleClass>
      <RoleClass Name="Buffering" RefBaseClassPath="MaterialHandlingProcessClassLib/Storing" ChangeMode="create">
        <Description>Buffering is a similar process to storing where transport units are kept in a buffer to ensure trouble-free operation.</Description>
      </RoleClass>
      <RoleClass Name="Retaining" RefBaseClassPath="MaterialHandlingProcessClassLib/Storing" ChangeMode="create">
        <Description>Retaining is a similar process to storage and buffering, where transport units are deliberately and intentionally kept in a store, but only in the short term, in contrast to storage.</Description>
      </RoleClass>
    </RoleClass>
  </RoleClassLib>
  <RoleClassLib Name="MaterialHandlingProductClassLib" ChangeMode="change">
    <Description>Role class library for material flow products and load carriers</Description>
    <Version>1.3</Version>
    <RoleClass Name="TransportUnit" RefBaseClassPath="AutomationMLBaseLibraries@AutomationMLBaseRoleClassLib/AutomationMLBaseRole/Product" ChangeMode="change">
      <Description>A moving object, which are handled by resources to create a final product, or the product itself</Description>
      <Version>1.1</Version>
      <Attribute Name="BasicForm" AttributeDataType="xs:string">
        <Description>Basic form of the product if the measurements are not defined accurately. Can be linear, laminar, cubic, cylindrical or free shape (undefined).</Description>
        <Constraint Name="FormCollection">
          <NominalScaledType>
            <RequiredValue>Linear</RequiredValue>
            <RequiredValue>Laminar</RequiredValue>
            <RequiredValue>Cubic</RequiredValue>
            <RequiredValue>Cylindrical </RequiredValue>
            <RequiredValue>FreeShape</RequiredValue>
          </NominalScaledType>
        </Constraint>
      </Attribute>
      <Attribute Name="Height" AttributeDataType="xs:double" Unit="m">
        <Description>Height of the transport unit</Description>
        <Attribute Name="Min" AttributeDataType="xs:double" Unit="m">
          <Description>Minimum height of the transport unit</Description>
        </Attribute>
        <Attribute Name="Max" AttributeDataType="xs:double" Unit="m">
          <Description>Maximum height of the transport unit</Description>
        </Attribute>
      </Attribute>
      <Attribute Name="Width" AttributeDataType="xs:double" Unit="m">
        <Description>Width of the transport unit</Description>
        <Attribute Name="Min" AttributeDataType="xs:double" Unit="m">
          <Description>Minimum width of the transport unit</Description>
        </Attribute>
        <Attribute Name="Max" AttributeDataType="xs:double" Unit="m">
          <Description>Maximum width of the transport unit</Description>
        </Attribute>
      </Attribute>
      <Attribute Name="Length" AttributeDataType="xs:double" Unit="m">
        <Description>Length of the transport unit</Description>
        <Attribute Name="Min" AttributeDataType="xs:double" Unit="m">
          <Description>Minimum length of the transport unit</Description>
        </Attribute>
        <Attribute Name="Max" AttributeDataType="xs:double" Unit="m">
          <Description>Maximum length of the transport unit</Description>
        </Attribute>
      </Attribute>
      <Attribute Name="Weight" AttributeDataType="xs:double" Unit="kg">
        <Description>Weight of the transport unit</Description>
        <Attribute Name="Min" AttributeDataType="xs:double" Unit="kg">
          <Description>Minimum weight of the transport unit</Description>
        </Attribute>
        <Attribute Name="Max" AttributeDataType="xs:double" Unit="kg">
          <Description>Maximum weight of the transport unit</Description>
        </Attribute>
      </Attribute>
      <Attribute Name="SpecialProperties" AttributeDataType="xs:string">
        <Description>Other properties which can be defined freely i.e. Shock sensitivity</Description>
      </Attribute>
    </RoleClass>
    <RoleClass Name="LoadCarrier" RefBaseClassPath="AutomationMLBaseLibraries@AutomationMLBaseRoleClassLib/AutomationMLBaseRole/Product" ChangeMode="change">
      <Description>A general role for load carriers like containers, pallets, boxes. They move together with transport units on the production line. It is also possible that they are delivered together with transport units in the end product. That is an element which has the characteristics of both the product and the resource.</Description>
      <Version>1.2</Version>
      <Attribute Name="Length" AttributeDataType="xs:double" Unit="m" ChangeMode="create">
        <Description>Length of the load carrier</Description>
      </Attribute>
      <Attribute Name="Width" AttributeDataType="xs:double" Unit="m" ChangeMode="create">
        <Description>Width of the load carrier</Description>
      </Attribute>
      <Attribute Name="Height" AttributeDataType="xs:double" Unit="m" ChangeMode="create">
        <Description>Height of the load carrier</Description>
      </Attribute>
      <RoleClass Name="Box" RefBaseClassPath="MaterialHandlingProductClassLib/LoadCarrier" ChangeMode="create">
        <Description>Represents box for materials</Description>
      </RoleClass>
      <RoleClass Name="Container" RefBaseClassPath="MaterialHandlingProductClassLib/LoadCarrier" ChangeMode="create">
        <Description>Represents any container for materials</Description>
      </RoleClass>
      <RoleClass Name="Pallet" RefBaseClassPath="MaterialHandlingProductClassLib/LoadCarrier" ChangeMode="create">
        <Description>Represents any pallet for materials</Description>
      </RoleClass>
      <RoleClass Name="OpenLoadCarrier" RefBaseClassPath="MaterialHandlingProductClassLib/LoadCarrier" ChangeMode="create">
        <Description>Represents a general open load carrier for materials like shelf boards, pallets, etc.</Description>
        <Version>1.0</Version>
      </RoleClass>
      <RoleClass Name="ClosedLoadCarrier" RefBaseClassPath="MaterialHandlingProductClassLib/LoadCarrier" ChangeMode="create">
        <Description>Represents a general closed load carrier for materials like containers, pallet cages, etc.</Description>
        <Version>1.0</Version>
      </RoleClass>
      <RoleClass Name="SpecificLoadCarrier" RefBaseClassPath="MaterialHandlingProductClassLib/LoadCarrier" ChangeMode="create">
        <Description>Represents a specific load carrier for materials like skids, racks, etc.</Description>
        <Version>1.0</Version>
        <Attribute Name="Type" AttributeDataType="xs:string">
          <Description>Type of the specific load carrier</Description>
        </Attribute>
      </RoleClass>
    </RoleClass>
  </RoleClassLib>
</CAEXFile>